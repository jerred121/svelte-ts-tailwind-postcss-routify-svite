    "@types/postcss-preset-env": "^6.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.0.6",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.2.1",
    "svelte": "3.25.1",
    "svelte-check": "^1.0.46",
    "svelte-hmr": "0.10.3",
    "svelte-preprocess": "4.3.0",
    "svite": "^0.7.0",
